<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobappdev.keypool">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_keypool"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Keypool">
        <activity android:name=".LoadingScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registerlogin.Register"
            android:parentActivityName=".registerlogin.Register">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".registerlogin.Register" />
        </activity>
        <activity android:name=".registerlogin.Login"
            android:parentActivityName=".registerlogin.Register">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".registerlogin.Register" />
        </activity>
        <activity android:name=".passwordlist.PasswordView"
            android:parentActivityName=".registerlogin.Login">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".registerlogin.Login" />
        </activity>
    </application>

</manifest>